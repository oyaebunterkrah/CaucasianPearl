@using CaucasianPearl.Models.EDM

@model Profile

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>User</legend>

    <div class="display-label">UserName</div>
    <div class="display-field">@Model.UserName</div>

    <div class="display-label">Email</div>
    <div class="display-field">@Model.Email</div>

    <div class="display-label">FirstName</div>
    <div class="display-field">@Model.FirstName</div>

    <div class="display-label">LastName</div>
    <div class="display-field">@Model.LastName</div>

    <div class="display-label">Group</div>
    <div class="display-field">@Model.Group</div>

    <div class="display-label">ImageExt</div>
    <div class="display-field">@Model.ImageExt</div>

    <div class="display-label">ShortName</div>
    <div class="display-field">@Model.ShortName</div>

    <div class="display-label">Sequence</div>
    <div class="display-field">@Model.Sequence</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


