@using CaucasianPearl.Core.Constants
@using CaucasianPearl.Core.UserControls
@using CaucasianPearl.Resources;

@model CaucasianPearl.Models.EDM.Feedback

@{
    ViewBag.Title = ViewRes.Edit;
}

<h2>@ViewRes.FeedbackEditing</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary()
    <fieldset>
        <legend>@ViewRes.Feedbacks</legend>

        @Html.HiddenFor(model => model.ID)

		@Html.Partial(Consts.Views.CreateOrEdit, Model)

        <input type="submit" value="@ViewRes.Save" />
    </fieldset>
}

<div>
    @Html.ActionLink(@ViewRes.BackToList, Consts.Actions.Index)
</div>
